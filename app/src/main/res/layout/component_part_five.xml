<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true"
    tools:ignore="SpeakableTextPresentCheck">

    <com.hcmute.finalproject.toeicapp.components.common.CommonHeaderComponent
        android:id="@+id/common_part_five_header_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="15dp" >

    </com.hcmute.finalproject.toeicapp.components.common.CommonHeaderComponent>

    <com.hcmute.finalproject.toeicapp.components.AnswerSelectionComponent
        android:id="@+id/component_part_five_answer_selection"
        android:layout_width="match_parent"
        android:layout_height="225dp"
        android:layout_below="@+id/common_part_five_header_title"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="50dp"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="447dp">

    </com.hcmute.finalproject.toeicapp.components.AnswerSelectionComponent>

    <com.hcmute.finalproject.toeicapp.components.common.CommonTestFooterComponent

        android:layout_width="match_parent"
        android:layout_height="52dp"
        android:layout_alignParentBottom="true"
        app:layout_constraintVertical_bias="0.988"
        tools:ignore="MissingConstraints" />
</RelativeLayout>